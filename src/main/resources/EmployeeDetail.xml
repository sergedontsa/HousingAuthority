<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.housing.authority.Tuples.Employee.EmployeeDetail">
        <table name="employee_detail" schema="vertical" catalog=""/>
        <attributes>
            <id name="detailId">
                <column name="detail_id" precision="10"/>
            </id>
            <basic name="firstName">
                <column name="first_name" nullable="false" length="50"/>
            </basic>
            <basic name="middleName">
                <column name="middle_name" nullable="false" length="50"/>
            </basic>
            <basic name="lastName">
                <column name="last_name" nullable="false" length="50"/>
            </basic>
            <basic name="email">
                <column name="email" nullable="false" length="50"/>
            </basic>
            <basic name="dateOfBirth">
                <column name="date_of_birth" nullable="false" length="50"/>
            </basic>
            <basic name="countryOfBirth">
                <column name="country_of_birth" nullable="false" length="50"/>
            </basic>
            <basic name="cityOfBirth">
                <column name="city_of_birth" nullable="false" length="50"/>
            </basic>
            <basic name="phoneNumber">
                <column name="phone_number" nullable="false" length="50"/>
            </basic>
            <basic name="registerdate">
                <column name="registerdate" nullable="false" precision="-1"/>
            </basic>
            <basic name="lastupdate">
                <column name="lastupdate" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
