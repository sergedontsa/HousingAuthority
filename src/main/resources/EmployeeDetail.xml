<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.housing.authority.Tuples.Employee.EmployeeDetail">
        <table name="employee_detail" schema="housingauthority" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="employeeid">
                <column name="employeeid" length="50"/>
            </basic>
            <basic name="firstname">
                <column name="firstname" length="50"/>
            </basic>
            <basic name="middlename">
                <column name="middlename" nullable="false" length="50"/>
            </basic>
            <basic name="lastname">
                <column name="lastname" length="50"/>
            </basic>
            <basic name="email">
                <column name="email" length="50"/>
            </basic>
            <basic name="phonenumber">
                <column name="phonenumber" length="50"/>
            </basic>
            <basic name="idtype">
                <column name="idtype" length="50"/>
            </basic>
            <basic name="idnumber">
                <column name="idnumber" length="50"/>
            </basic>
            <basic name="issuedate">
                <column name="issuedate" length="50"/>
            </basic>
            <basic name="expiredate">
                <column name="expiredate" length="50"/>
            </basic>
            <basic name="nationality">
                <column name="nationality" length="50"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
