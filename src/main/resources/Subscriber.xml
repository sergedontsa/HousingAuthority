<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.housing.authority.Tuples.Subscriber">
        <table name="subscriber" schema="housingauthority" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="name">
                <column name="name" length="50"/>
            </basic>
            <basic name="email">
                <column name="email" length="50"/>
            </basic>
            <basic name="registerdate">
                <column name="registerdate" nullable="false" length="50"/>
            </basic>
            <basic name="lastupdate">
                <column name="lastupdate" nullable="false" length="50"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
