<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.housing.authority.Tuples.Employee.EmployeeAddress">
        <table name="employee_address" schema="vertical" catalog=""/>
        <attributes>
            <id name="addressId">
                <column name="address_id" precision="10"/>
            </id>
            <basic name="number">
                <column name="number" nullable="false" length="10"/>
            </basic>
            <basic name="streetName">
                <column name="street_name" nullable="false" length="50"/>
            </basic>
            <basic name="unit">
                <column name="unit" nullable="false" length="10"/>
            </basic>
            <basic name="city">
                <column name="city" nullable="false" length="50"/>
            </basic>
            <basic name="province">
                <column name="province" nullable="false" length="50"/>
            </basic>
            <basic name="zipCode">
                <column name="zip_code" nullable="false" length="20"/>
            </basic>
            <basic name="country">
                <column name="country" nullable="false" length="50"/>
            </basic>
            <basic name="registerdate">
                <column name="registerdate" nullable="false" precision="-1"/>
            </basic>
            <basic name="lastupdate">
                <column name="lastupdate" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
