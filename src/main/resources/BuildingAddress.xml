<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.housing.authority.Tuples.Building.BuildingAddress">
        <table name="building_address" schema="housingauthority" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="buildingid">
                <column name="buildingid" length="50"/>
            </basic>
            <basic name="number">
                <column name="number" nullable="false" precision="20"/>
            </basic>
            <basic name="city">
                <column name="city" length="50"/>
            </basic>
            <basic name="province">
                <column name="province" length="50"/>
            </basic>
            <basic name="zipcode">
                <column name="zipcode" length="50"/>
            </basic>
            <basic name="country">
                <column name="country" length="50"/>
            </basic>
            <basic name="registerdate">
                <column name="registerdate" precision="-1"/>
            </basic>
            <basic name="lastupdate">
                <column name="lastupdate" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
